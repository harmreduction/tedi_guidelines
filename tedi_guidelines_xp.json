{
    "date": {
        "VARIABLE_NAME": "Date (yyyy-mm-dd)",
        "DESCRIPTION": "Date of the analysis",
        "TYPE": "timestamp",
        "REQUIREMENT_LEVEL": "required"
    },
    "organisation": {
        "VARIABLE_NAME": "Organisation",
        "DESCRIPTION": "The name of the organisation collecting the data",
        "TYPE": "text",
        "REQUIREMENT_LEVEL": "required"
    },
    "sample_uid": {
        "VARIABLE_NAME": "Sample UID",
        "DESCRIPTION": "A unique ID value for the sample which will never be re-used for more than one sample",
        "TYPE": "uuid",
        "REQUIREMENT_LEVEL": "required"
    },
    "country": {
        "VARIABLE_NAME": "Country",
        "DESCRIPTION": "The country of data collection",
        "TYPE": "text",
        "REQUIREMENT_LEVEL": "required"
    },
    "city": {
        "VARIABLE_NAME": "City",
        "DESCRIPTION": "The city where the sample was collected from. If this is not available, the city that the sample was tested in should be given.",
        "TYPE": "text",
        "REQUIREMENT_LEVEL": "required"
    },
    "geo_context": {
        "VARIABLE_NAME": "Geography/Context (where sample was purchased)",
        "DESCRIPTION": "A list of geographic contexts from which a sample could originate. E.g. \u201cA music festival\u201d, \u201ca street\u201d,",
        "TYPE": "controlled vocabulary",
        "VOCABULARY": [],
        "REQUIREMENT_LEVEL": "recommended"
    },
    "provider_relation": {
        "VARIABLE_NAME": "Relationship with provider",
        "DESCRIPTION": "A list of options, such as \u201ca friend\u201d, \u201ca stranger\u201d, \u201conline\u201d. This is more important than the geographic context.",
        "TYPE": "controlled vocabulary",
        "VOCABULARY": [],
        "REQUIREMENT_LEVEL": "recommended"
    },
    "sold_as": {
        "VARIABLE_NAME": "Sold-As",
        "DESCRIPTION": "What the substance was said to be when it was bought or given to the service user",
        "TYPE": "text",
        "REQUIREMENT_LEVEL": "required"
    },
    "alias": {
        "VARIABLE_NAME": "Alias (optional)",
        "DESCRIPTION": "A slang name that the service user used (if any)",
        "TYPE": "text",
        "REQUIREMENT_LEVEL": "optional"
    },
    "used_prior_test": {
        "VARIABLE_NAME": "Sample used prior to test",
        "DESCRIPTION": "Whether the sample was consumed by the service user or a close friend before submitting for testing",
        "TYPE": "bool",
        "REQUIREMENT_LEVEL": "recommended"
    },
    "sample_form": {
        "VARIABLE_NAME": "Sample Form",
        "DESCRIPTION": "A fixed list of options (tablet, powder, liquid etc)",
        "TYPE": "text",
        "REQUIREMENT_LEVEL": "required"
    },
    "colour": {
        "VARIABLE_NAME": "Colour",
        "DESCRIPTION": NaN,
        "TYPE": "text",
        "REQUIREMENT_LEVEL": "required"
    },
    "logo": {
        "VARIABLE_NAME": "Logo",
        "DESCRIPTION": "(Tablets/blotter, not powders etc)",
        "TYPE": "text",
        "REQUIREMENT_LEVEL": "recommended"
    },
    "width": {
        "VARIABLE_NAME": "Width (mm)",
        "DESCRIPTION": "(Tablets/blotter, not powders etc)",
        "TYPE": "number",
        "REQUIREMENT_LEVEL": "recommended"
    },
    "thickness": {
        "VARIABLE_NAME": "Thickness (mm)",
        "DESCRIPTION": "(Tablets/blotter, not powders etc)",
        "TYPE": "number",
        "REQUIREMENT_LEVEL": "recommended"
    },
    "height": {
        "VARIABLE_NAME": "Height (mm)",
        "DESCRIPTION": "(Tablets/blotter, not powders etc)",
        "TYPE": "number",
        "REQUIREMENT_LEVEL": "recommended"
    },
    "weight": {
        "VARIABLE_NAME": "Weight (mg)",
        "DESCRIPTION": "(Tablets/blotter, not powders etc)",
        "TYPE": "number",
        "REQUIREMENT_LEVEL": "recommended"
    },
    "unit_price": {
        "VARIABLE_NAME": "Price per unit",
        "DESCRIPTION": NaN,
        "TYPE": "number",
        "REQUIREMENT_LEVEL": "recommended"
    },
    "price_currency": {
        "VARIABLE_NAME": "Price currency",
        "DESCRIPTION": NaN,
        "TYPE": "text",
        "REQUIREMENT_LEVEL": "recommended"
    },
    "gender": {
        "VARIABLE_NAME": "Gender",
        "DESCRIPTION": NaN,
        "TYPE": "controlled vocabulary",
        "VOCABULARY": ["M", "F", "NB"],
        "REQUIREMENT_LEVEL": "recommended"
    },
    "age": {
        "VARIABLE_NAME": "Age",
        "DESCRIPTION": NaN,
        "TYPE": "number",
        "REQUIREMENT_LEVEL": "recommended"
    },
    "test_method": {
        "VARIABLE_NAME": "Test Method",
        "DESCRIPTION": "The analytical method used to obtain the result\n(if multiple methods used, enter most sophisticated)",
        "TYPE": "controlled vocabulary",
        "VOCABULARY": ["ANTIBODY TEST STRIPS",
             "REAGENT TESTING",
             "TLC",
             "UV SPECTROSCOPY",
             "FTIR/ RAMAN",
             "(U)HPLC-UV",
             "(U)HPLC-MS ",
             "GC-MS",
             "DIRECT MS",
             "LC-HRMS"],
        "REQUIREMENT_LEVEL": "required"
    },
    "service_type": {
        "VARIABLE_NAME": "Service Type",
        "DESCRIPTION": "The context of the service e.g. \u201cmusic event\u201d or \u201csafer consumption facility\u201d",
        "TYPE": "controlled vocabulary",
        "VOCABULARY": [],
        "REQUIREMENT_LEVEL": "recommended"
    },
    "substance_1": {
        "VARIABLE_NAME": "Substance-1",
        "DESCRIPTION": "The identity of the first substance detected",
        "TYPE": "text",
        "REQUIREMENT_LEVEL": "required"
    },
    "subs1_quant": {
        "VARIABLE_NAME": "Subst1-Quant",
        "DESCRIPTION": "The numerical value of the quantification (if quantified). Excludes units, numerical data only",
        "TYPE": "number",
        "REQUIREMENT_LEVEL": "required"
    },
    "subs1_unit": {
        "VARIABLE_NAME": "Subst1-Unit",
        "DESCRIPTION": "The units of the quantification (if quantified)",
        "TYPE": "text",
        "REQUIREMENT_LEVEL": "required"
    },
    "substance_9": {
        "VARIABLE_NAME": "Substance-9",
        "DESCRIPTION": "There are 9 columns in total, to allow detection of mixtures. For simplicity, 2-8 are not shown in this document",
        "TYPE": "text",
        "REQUIREMENT_LEVEL": "optional"
    },
    "subs9_quant": {
        "VARIABLE_NAME": "Subst9-Quant",
        "DESCRIPTION": NaN,
        "TYPE": "number",
        "REQUIREMENT_LEVEL": "optional"
    },
    "subs9_unit": {
        "VARIABLE_NAME": "Subst9-Unit",
        "DESCRIPTION": NaN,
        "TYPE": "text",
        "REQUIREMENT_LEVEL": "optional"
    },
    "alert": {
        "VARIABLE_NAME": "Alert",
        "DESCRIPTION": "If an alert was issued by the testing organisation",
        "TYPE": "bool",
        "REQUIREMENT_LEVEL": "required"
    },
    "image": {
        "VARIABLE_NAME": "Image",
        "DESCRIPTION": "Image of the sample",
        "TYPE": "binary",
        "REQUIREMENT_LEVEL": "optional"
    }
}